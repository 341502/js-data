{
  "source": {
    "includePattern": ".*js$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "./node_modules/ink-docstrap/template",
    "destination": "./doc/",
    "recurse": true,
    "verbose": true,
    "readme": "./README.md",
    "package": "./package.json"
  },
  "templates": {
    "theme": "cosmo",
    "systemName": "js-data",
    "copyright": "js-data Copyright Â© 2014-2015 Jason Dobry"
  },
  "source": {
    "excludePattern": "(sync_methods|old_utils)"
  }
}